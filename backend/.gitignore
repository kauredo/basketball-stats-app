# Backend .gitignore (Ruby on Rails)

# Ignore bundler config
/.bundle

# Ignore all logfiles and tempfiles
/log/*
./tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials
/config/master.key

# Ignore database configuration
/config/database.yml

# Ignore environment variables
.env*
!.env.example

# Ignore compiled assets
/public/assets
/public/packs
/public/packs-test

# Ignore yarn files
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore Ruby version
.ruby-version

# Ignore node_modules
/node_modules

# Ignore coverage data
/coverage

# Ignore precompiled javascript packs
/public/packs
/public/packs-test
/public/assets

# Ignore JavaScript bundle
/app/assets/builds

# Ignore vendor bundle
/vendor/bundle

# Ignore local database dumps
*.dump
*.sql

# Ignore RSpec state
spec/examples.txt

# Ignore SimpleCov generated files
/coverage/
