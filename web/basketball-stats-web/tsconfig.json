{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    /* Web-specific: Vite handles bundling, we just typecheck */
    "noEmit": true,
    /* Target ES5 for broader browser support */
    "target": "ES5",
    /* Use node resolution for Vite compatibility */
    "moduleResolution": "node"
  },
  "include": ["src"]
}
